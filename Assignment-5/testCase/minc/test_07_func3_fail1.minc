func mul(var int a,var int b)->int
{
    if( a = 0 )
        return 0;
    else {
        if( b < 0 )
            return call mul(a, 11) - a;
        else
            return call mul(a, b-1) + a;
    }
}
func main()->int
{
    var   int    xyz ;
    xyz <- call mul(true, 5);
    return 0;
}
